{
    "name": "TSReact",
    "version": "0.0.1",
    "description": "TypeScript React and Redux with Express SSR starting point",
    "main": "src/index.ts",
    "repository": "git@github.com:WsCandy/TSReact.git",
    "author": "Samuel Woodbridge <s-bridge@live.co.uk>",
    "license": "MIT",
    "scripts": {
        "start": "NODE_ENV=production node dist/index.js --assets=true",
        "prebuild": "yarn clean",
        "build": "concurrently \"yarn:build:*\"",
        "build:server": "NODE_ENV=production webpack --config ./.webpack/server.js",
        "build:client": "NODE_ENV=production webpack --config ./.webpack/client.js",
        "predev": "yarn clean && mkdir -p dist && touch dist/index.js",
        "dev": "concurrently \"yarn:dev:*\"",
        "dev:client": "webpack-dev-server --inline --hot --config ./.webpack/client.js --colors",
        "dev:server": "webpack --config ./.webpack/server.js --watch --colors & nodemon watch dist/index.js --exec \"node dist/index.js\"",
        "clean": "concurrently \"yarn:clean:*\"",
        "clean:dist": "rimraf dist/*",
        "clean:cache": "rimraf .cache-loader/*",
        "lint": "concurrently \"yarn:lint:*\"",
        "lint:ts": "eslint --ext .ts --ext .tsx src/",
        "lint:cs": "stylelint \"src/**/*.{tsx,ts}\" --color",
        "pretty": "prettier src/**/*.ts --write"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix --color",
            "git add",
            "stylelint --color"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@types/express": "^4.16.0",
        "@types/html-entities": "^1.2.16",
        "@types/i18next-express-middleware": "^0.0.33",
        "@types/lodash": "^4.14.133",
        "@types/react": "^16.7.17",
        "@types/react-dom": "^16.0.11",
        "@types/react-loadable": "^5.4.3",
        "@types/react-redux": "^6.0.11",
        "@types/react-router": "^4.4.3",
        "@types/react-router-dom": "^4.3.1",
        "@types/react-transition-group": "^2.0.16",
        "@types/redux-batched-actions": "^0.1.5",
        "@types/redux-thunk": "^2.1.0",
        "@types/styled-components": "^4.1.4",
        "@types/webpack": "^4.4.21",
        "@types/webpack-env": "^1.13.6",
        "@types/yargs": "^12.0.1",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-styled-components": "^1.10.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "cache-loader": "^1.2.5",
        "concurrently": "^4.1.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.0.2",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-import-resolver-webpack": "^0.11.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.11.1",
        "fork-ts-checker-notifier-webpack-plugin": "^1.0.0-alpha.0",
        "fork-ts-checker-webpack-plugin": "^1.0.1",
        "html-webpack-harddisk-plugin": "^1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-webpack-plugin": "^2.4.2",
        "lint-staged": "^8.1.0",
        "nodemon": "^1.18.9",
        "postcss-discard-comments": "^4.0.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.15.3",
        "pretty-quick": "^1.8.0",
        "raw-loader": "^1.0.0",
        "responsive-loader": "^1.2.0",
        "script-ext-html-webpack-plugin": "^2.1.3",
        "sharp": "^0.22.1",
        "stylelint": "^9.9.0",
        "stylelint-config-recommended": "^2.1.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.5.1",
        "svgo": "^1.1.1",
        "svgo-loader": "^2.2.0",
        "to-string-loader": "^1.1.5",
        "ts-loader": "^5.3.1",
        "typescript": "3.3",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.28.0",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "webpack-merge": "^4.1.5",
        "webpack-node-externals": "^1.7.2",
        "workbox-webpack-plugin": "^4.3.1"
    },
    "dependencies": {
        "body-scroll-lock": "^2.6.1",
